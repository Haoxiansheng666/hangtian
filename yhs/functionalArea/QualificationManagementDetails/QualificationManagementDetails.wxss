.navSub {
  width: 70rpx;
  text-align: center;
  margin: auto 42rpx;
  margin-top: 29rpx !important;
  font-size: 28rpx;
  font-family: OPPOSans;
  font-weight: 400;
  color: #333333;
}

.nav_max {
  background-color: white;
}

.navSub_split_ {
  opacity: 0;
}

.Select_ {
  font-size: 32rpx !important;
  font-weight: bold !important;
  color: #181818 !important;
}

.navSub_split view {
  width: 29rpx;
  height: 7rpx;
  background: #1684fc;
  display: inline-block;
  position: absolute;
  left: 35%;
  top: -6rpx;
}

.navSub_split {
  width: 70rpx;
  margin: auto 42rpx;
  margin-top: 20rpx !important;
  position: relative;
}

.nav_box {
  display: flex;
  align-content: center;
}

page {
  background-color: #f6f6f6;
}

.cfc_box {
  width: 690rpx;
  background-color: white;
  margin: 29rpx auto;
  border-radius: 10rpx !important;
  padding: 15rpx 0;
}
.cfc_box .subject {
  font-size: 28rpx;
  font-family: OPPOSans-R;
  font-weight: Regular;
  color: #333333;
  height: 70rpx;
  line-height: 70rpx;
  margin-left: 39rpx;
}
.file {
  width: 610rpx;
  height: 137rpx;
  background-color: white;
  border-radius: 10rpx;
  margin: auto;
  padding: 40rpx;
}
.file .fl_c {
  width: 84rpx;
  height: 84rpx;
  background: #f6f6f6;
  border-radius: 10rpx;
  text-align: center;
  margin-top: 30rpx;
}
.file .fl_c image {
  width: 38rpx;
  height: 46rpx;
  margin-top: 20rpx;
}
.file .fl_t {
  font-size: 28rpx;
  font-family: OPPOSans;
  font-weight: 400;
  color: #333333;
}


.bf {
  width: 690rpx;
  margin: auto;
  margin-top: 31rpx !important;
  background-color: white;
  border-radius: 10rpx;
  padding-bottom: 45rpx;
}

.bf .brief {
  font-weight: 500;
  color: #333333;
  padding: 40rpx 0 0 36rpx;
  font-size: 32rpx;
  font-family: OPPOSans;
  font-weight: bold;
  color: #333333;
}

.bf .bfd_ep {
  color: #1684FC;
}

.bf .brief_ds {
  width: 620rpx;
  font-size: 28rpx;
  font-family: OPPOSans;
  font-weight: 400;
  color: #666666;
  margin-top: 29rpx;
  margin-left: 35rpx;
}