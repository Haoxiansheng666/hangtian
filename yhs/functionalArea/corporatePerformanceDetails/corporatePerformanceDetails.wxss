.cfc_box {
  width: 690rpx;
  background-color: white;
  margin: 29rpx auto;
  border-radius: 10rpx;
  padding: 15rpx 0;
}
.cfc_box .subject {
  font-size: 28rpx;
  font-family: OPPOSans-R;
  font-weight: Regular;
  color: #333333;
  height: 70rpx;
  line-height: 70rpx;
  margin-left: 39rpx;
}
page {
  background-color: #f6f6f6;
}
.file {
  width: 610rpx;
  height: 137rpx;
  background-color: white;
  border-radius: 10rpx;
  margin: auto;
  padding: 40rpx;
}
.file .fl_c {
  width: 84rpx;
  height: 84rpx;
  background: #f6f6f6;
  border-radius: 10rpx;
  text-align: center;
  margin-top: 30rpx;
}
.file .fl_c image {
  width: 38rpx;
  height: 46rpx;
  margin-top: 20rpx;
}
.file .fl_t {
  font-size: 28rpx;
  font-family: OPPOSans;
  font-weight: 400;
  color: #333333;
}
